{
  "name": "interiora-full-stack",
  "private": true,
  "version": "1.0.0",
  "description": "Interiora Full Stack Application - Interior Design Platform",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "start": "npm run start:backend",
    "deploy:build": "npm run install:all && npm run build:frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["interior-design", "react", "express", "mongodb", "full-stack"],
  "author": "Interiora Team",
  "license": "MIT"
}
